<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="ApplicationCore" namespace="ApplicationCore.Domain.EN">
  <class name="ChatEquipo" table="ChatEquipo">
    <id name="IdChatEquipo" column="IdChatEquipo" type="long">
      <generator class="hilo">
        <param name="table">NHibernateUniqueKey</param>
        <param name="column">NextHigh</param>
        <param name="max_lo">100</param>
      </generator>
    </id>
    
    <one-to-one name="Equipo" class="Equipo" />
    <bag name="Mensajes" inverse="true" cascade="all">
      <key column="ChatId" />
      <one-to-many class="MensajeChat" />
    </bag>
  </class>
</hibernate-mapping>
