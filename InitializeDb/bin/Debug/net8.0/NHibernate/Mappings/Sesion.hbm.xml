<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="ApplicationCore" namespace="ApplicationCore.Domain.EN">
  <class name="Sesion" table="Sesion">
    <id name="IdSesion" column="IdSesion" type="long">
      <generator class="hilo">
        <param name="table">NHibernateUniqueKey</param>
        <param name="column">NextHigh</param>
        <param name="max_lo">100</param>
      </generator>
    </id>
    <property name="FechaInicio" column="FechaInicio" type="datetime" not-null="true" />
    <property name="FechaFin" column="FechaFin" type="datetime" />
    <property name="Token" column="Token" type="string" not-null="true" />
    
    <many-to-one name="Usuario" class="Usuario" column="UsuarioId" not-null="true" />
  </class>
</hibernate-mapping>
