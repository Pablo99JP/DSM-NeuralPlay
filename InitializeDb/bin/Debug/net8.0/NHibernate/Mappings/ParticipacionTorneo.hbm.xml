<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="ApplicationCore" namespace="ApplicationCore.Domain.EN">
  <class name="ParticipacionTorneo" table="ParticipacionTorneo">
    <id name="IdParticipacion" column="IdParticipacion" type="long">
      <generator class="hilo">
        <param name="table">NHibernateUniqueKey</param>
        <param name="column">NextHigh</param>
        <param name="max_lo">100</param>
      </generator>
    </id>
    <property name="Estado" column="Estado" type="ApplicationCore.Domain.Enums.EstadoParticipacion, ApplicationCore" not-null="true" />
    <property name="FechaAlta" column="FechaAlta" type="datetime" not-null="true" />
    
    <many-to-one name="Equipo" column="EquipoId" class="Equipo" not-null="true" />
    <many-to-one name="Torneo" column="TorneoId" class="Torneo" not-null="true" />
  </class>
</hibernate-mapping>
