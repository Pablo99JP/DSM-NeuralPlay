<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="ApplicationCore" namespace="ApplicationCore.Domain.EN">
  <class name="MiembroComunidad" table="MiembroComunidad">
    <id name="IdMiembroComunidad" column="IdMiembroComunidad" type="long">
      <generator class="hilo">
        <param name="table">NHibernateUniqueKey</param>
        <param name="column">NextHigh</param>
        <param name="max_lo">100</param>
      </generator>
    </id>
    <property name="Rol" column="Rol" type="ApplicationCore.Domain.Enums.RolComunidad, ApplicationCore" not-null="true" />
    <property name="Estado" column="Estado" type="ApplicationCore.Domain.Enums.EstadoMembresia, ApplicationCore" not-null="true" />
    <property name="FechaAlta" column="FechaAlta" type="datetime" not-null="true" />
    <property name="FechaBaja" column="FechaBaja" type="datetime" />
    
    <many-to-one name="Usuario" class="Usuario" column="UsuarioId" not-null="true" />
    <many-to-one name="Comunidad" class="Comunidad" column="ComunidadId" not-null="true" />
  </class>
</hibernate-mapping>
