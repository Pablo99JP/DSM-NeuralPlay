<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="ApplicationCore" namespace="ApplicationCore.Domain.EN">
  <class name="Juego" table="Juego">
    <id name="IdJuego" column="IdJuego" type="long">
      <generator class="hilo">
        <param name="table">NHibernateUniqueKey</param>
        <param name="column">NextHigh</param>
        <param name="max_lo">100</param>
      </generator>
    </id>
    <property name="NombreJuego" column="NombreJuego" type="string" not-null="true" />
    <property name="Genero" column="Genero" type="ApplicationCore.Domain.Enums.GeneroJuego, ApplicationCore" not-null="true" />
    
    <bag name="PerfilJuegos" inverse="true" cascade="all">
      <key column="JuegoId" />
      <one-to-many class="PerfilJuego" />
    </bag>
  </class>
</hibernate-mapping>
