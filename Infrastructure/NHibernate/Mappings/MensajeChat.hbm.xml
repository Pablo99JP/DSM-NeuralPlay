<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="ApplicationCore" namespace="ApplicationCore.Domain.EN">
  <class name="MensajeChat" table="MensajeChat">
    <id name="IdMensajeChat" column="IdMensajeChat" type="long">
      <generator class="hilo">
        <param name="table">NHibernateUniqueKey</param>
        <param name="column">NextHigh</param>
        <param name="max_lo">100</param>
      </generator>
    </id>
    <property name="Contenido" column="Contenido" type="string" not-null="true" />
    <property name="FechaEnvio" column="FechaEnvio" type="datetime" not-null="true" />
    
    <many-to-one name="Chat" column="ChatId" class="ChatEquipo" not-null="true" />
    <many-to-one name="Autor" column="AutorId" class="Usuario" not-null="true" />
  </class>
</hibernate-mapping>
