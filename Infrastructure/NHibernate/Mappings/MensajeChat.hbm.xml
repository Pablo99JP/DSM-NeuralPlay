<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//NHibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
  <class name="ApplicationCore.Domain.EN.MensajeChat, ApplicationCore" table="MensajeChat">
    <id name="IdMensajeChat" column="IdMensajeChat" type="long">
      <generator class="native" />
    </id>
    <property name="Contenido" column="Contenido" />
    <property name="FechaEnvio" column="FechaEnvio" />

    <many-to-one name="Chat" class="ApplicationCore.Domain.EN.ChatEquipo, ApplicationCore" column="ChatId" />
    <many-to-one name="Autor" class="ApplicationCore.Domain.EN.Usuario, ApplicationCore" column="AutorId" />
  </class>
</hibernate-mapping>
