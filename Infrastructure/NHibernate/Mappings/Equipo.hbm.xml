<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//NHibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
  <class name="ApplicationCore.Domain.EN.Equipo, ApplicationCore" table="Equipo">
    <id name="IdEquipo" column="IdEquipo" type="long">
      <generator class="native" />
    </id>
    <property name="Nombre" column="Nombre" />
    <property name="Descripcion" column="Descripcion" />
    <property name="FechaCreacion" column="FechaCreacion" />

    <many-to-one name="Comunidad" class="ApplicationCore.Domain.EN.Comunidad, ApplicationCore" column="ComunidadId" />
  </class>
</hibernate-mapping>
