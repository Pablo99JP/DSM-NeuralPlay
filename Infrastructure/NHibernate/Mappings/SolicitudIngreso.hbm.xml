<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//NHibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
  <class name="ApplicationCore.Domain.EN.SolicitudIngreso, ApplicationCore" table="SolicitudIngreso">
    <id name="IdSolicitud" column="IdSolicitud" type="long">
      <generator class="native" />
    </id>
    <property name="Tipo" column="Tipo" />
    <property name="Estado" column="Estado" />
    <property name="FechaSolicitud" column="FechaSolicitud" />
    <property name="FechaResolucion" column="FechaResolucion" />

    <many-to-one name="Solicitante" class="ApplicationCore.Domain.EN.Usuario, ApplicationCore" column="SolicitanteId" />
    <many-to-one name="Comunidad" class="ApplicationCore.Domain.EN.Comunidad, ApplicationCore" column="ComunidadId" />
    <many-to-one name="Equipo" class="ApplicationCore.Domain.EN.Equipo, ApplicationCore" column="EquipoId" />
  </class>
</hibernate-mapping>
