<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//NHibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
  <class name="ApplicationCore.Domain.EN.Sesion, ApplicationCore" table="Sesion">
    <id name="IdSesion" column="IdSesion" type="long">
      <generator class="native" />
    </id>
    <property name="FechaInicio" column="FechaInicio" />
    <property name="FechaFin" column="FechaFin" />
    <property name="Token" column="Token" />

    <many-to-one name="Usuario" class="ApplicationCore.Domain.EN.Usuario, ApplicationCore" column="UsuarioId" />
  </class>
</hibernate-mapping>
