<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//NHibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
  <class name="ApplicationCore.Domain.EN.MiembroEquipo, ApplicationCore" table="MiembroEquipo">
    <id name="IdMiembroEquipo" column="IdMiembroEquipo" type="long">
      <generator class="native" />
    </id>
    <property name="Rol" column="Rol" />
    <property name="Estado" column="Estado" />
    <property name="FechaAlta" column="FechaAlta" />
    <property name="FechaBaja" column="FechaBaja" />

    <many-to-one name="Usuario" class="ApplicationCore.Domain.EN.Usuario, ApplicationCore" column="UsuarioId" />
    <many-to-one name="Equipo" class="ApplicationCore.Domain.EN.Equipo, ApplicationCore" column="EquipoId" />
  </class>
</hibernate-mapping>
