<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//NHibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
  <class name="ApplicationCore.Domain.EN.Usuario, ApplicationCore" table="Usuario">
    <id name="IdUsuario" column="IdUsuario" type="long">
      <generator class="hilo">
        <param name="table">NHibernateUniqueKey</param>
        <param name="column">NextHigh</param>
        <param name="max_lo">100</param>
      </generator>
    </id>
    <property name="Nick" column="Nick" />
    <property name="CorreoElectronico" column="CorreoElectronico" />
    <property name="ContrasenaHash" column="ContrasenaHash" />
    <property name="Telefono" column="Telefono" />
    <property name="FechaRegistro" column="FechaRegistro" />
    <property name="EstadoCuenta" column="EstadoCuenta" />
  </class>
</hibernate-mapping>
