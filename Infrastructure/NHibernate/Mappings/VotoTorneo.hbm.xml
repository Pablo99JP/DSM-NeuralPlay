<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="ApplicationCore" namespace="ApplicationCore.Domain.EN">
  <class name="VotoTorneo" table="VotoTorneo">
    <id name="IdVoto" column="IdVoto" type="long">
      <generator class="hilo">
        <param name="table">NHibernateUniqueKey</param>
        <param name="column">NextHigh</param>
        <param name="max_lo">100</param>
      </generator>
    </id>
    <property name="Valor" column="Valor" type="boolean" not-null="true" />
    <property name="FechaVoto" column="FechaVoto" type="datetime" not-null="true" />
    
    <many-to-one name="Propuesta" column="PropuestaId" class="PropuestaTorneo" not-null="true" />
    <many-to-one name="Votante" column="VotanteId" class="Usuario" not-null="true" />
  </class>
</hibernate-mapping>
