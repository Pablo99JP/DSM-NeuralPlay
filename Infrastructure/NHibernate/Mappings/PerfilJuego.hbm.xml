<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="ApplicationCore" namespace="ApplicationCore.Domain.EN">
  <class name="PerfilJuego" table="PerfilJuego">
    <id name="IdPerfilJuego" column="IdPerfilJuego" type="long">
      <generator class="hilo">
        <param name="table">NHibernateUniqueKey</param>
        <param name="column">NextHigh</param>
        <param name="max_lo">100</param>
      </generator>
    </id>
    <property name="FechaAdicion" column="FechaAdicion" type="datetime" not-null="true" />
    
    <many-to-one name="Perfil" column="PerfilId" class="Perfil" not-null="true" />
    <many-to-one name="Juego" column="JuegoId" class="Juego" not-null="true" />
  </class>
</hibernate-mapping>
