<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//NHibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
  <class name="ApplicationCore.Domain.EN.ParticipacionTorneo, ApplicationCore" table="ParticipacionTorneo">
    <id name="IdParticipacion" column="IdParticipacion" type="long">
      <generator class="native" />
    </id>
  <property name="FechaAlta" column="FechaAlta" />
    <property name="Estado" column="Estado" />
    <property name="Posicion" column="Posicion" />

    <many-to-one name="Equipo" class="ApplicationCore.Domain.EN.Equipo, ApplicationCore" column="EquipoId" />
    <many-to-one name="Torneo" class="ApplicationCore.Domain.EN.Torneo, ApplicationCore" column="TorneoId" />
  </class>
</hibernate-mapping>
